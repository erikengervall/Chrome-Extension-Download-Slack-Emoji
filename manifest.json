{
  "manifest_version": 3,
  "name": "One click emoji downloader for Slack",
  "description": "Downloads slack emojis in one click",
  "version": "1",
  "permissions": ["downloads"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://*/*"]
    }
  ]
}
